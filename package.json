{
  "name": "sbtci-interactive",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "d3-format": "^1.4.2",
    "d3-geo": "^1.11.9",
    "d3-interpolate": "^1.4.0",
    "d3-scale": "^3.2.1",
    "d3-scale-chromatic": "^1.5.0",
    "jsonp": "^0.2.1",
    "normalize.css": "^8.0.1",
    "npm-run-all": "^4.0.2",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "react-helmet": "^5.2.1",
    "react-router-dom": "^5.1.2",
    "react-scripts": "3.4.1",
    "react-snapshot": "^1.2.0",
    "react-transition-group": "^4.3.0",
    "topojson-client": "^3.1.0",
    "validate.js": "^0.13.1",
    "yarn": "^1.22.17"
  },
  "devDependencies": {
    "eslint": "^6.7.2",
    "eslint-plugin-react": "^7.17.0",
    "node-sass": "^4.13.1",
    "postcss": "^7.0.24"
  },
  "scripts": {
    "watch-sass": "node-sass src/style.scss src/style.css -w --source-map=src/style.css.map",
    "start-js": "react-scripts start",
    "start": "npm run build-sass && npm-run-all -p watch-sass start-js",
    "build-sass": "node-sass src/style.scss src/style.css --output-style=compressed",
    "build-js": "react-scripts build && react-snapshot",
    "build": "npm run build-sass && npm run build-js",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
